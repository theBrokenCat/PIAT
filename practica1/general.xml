                                <tipoActuacion></tipoActuacion>
                                <fechasActuacion></fechasActuacion>
                            </alcanceActuacion>
                        </actuador>
                    </actuadores>
                </dro:configuracionDron>
            </mis:dron>
        </mis:drones>
        <tareas>
            <tarea idTarea="">
                <descripcionTarea></descripcionTarea>
                <responsableTarea>
                    <url></url>
