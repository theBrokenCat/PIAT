<?xml version="1.0" encoding="UTF-8"?>
<tns:generalMision xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xmlns:ide="http://www.piat.upm.es/identificacionSchema"
            xmlns:dro="http://www.piat.upm.es/dronSchema"
            xmlns:sen="http://www.piat.upm.es/sensoresSchema"
            xmlns:act="http://www.piat.upm.es/actuadoresSchema"
            xmlns:tar="http://www.piat.upm.es/tareaSchema"
            xmlns:mis="http://www.piat.upm.es/misionSchema"

            xmlns:tns="http://www.piat.upm.es/generalSchema"
            xsi:schemaLocation="http://www.piat.upm.es/generalSchema ./generalSchema.xsd">
    <tns:mision>
        <mis:identificacion codigoMision="M123ab">
            <descripcion>
                <keyword></keyword>
            </descripcion>
        </mis:identificacion>
        <mis:drones>
            <mis:dron matriculaDron="">
                <dro:claseDron></dro:claseDron>
