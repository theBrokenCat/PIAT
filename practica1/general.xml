                                        <contacto>
                                            <email></email>
                                            <url></url>
                                        </contacto>
                                    </propietario>
                                </metadatos>
                                <datosMision idDM="">
                                    <descripcionDM></descripcionDM>
                                    <formatoDatos>XML</formatoDatos>
                                    <lenguaje></lenguaje>
                                    <fecha></fecha>
                                    <datosSensores></datosSensores>
                                    <datosActuadores></datosActuadores>
                                    <imagenes idImagen="">
                                        <datosImagen>
                                            <dimensiones>
                                                <ancho></ancho>
                                                <alto></alto>
                                            </dimensiones>
                                            <descripcion></descripcion>
                                            <contenido></contenido>
                                        </datosImagen>
                                    </imagenes>
                                </datosMision>
                            </documentoMision>
                            <mision>
                            </mision>
                            <imagen idImagen="">
                            </imagen>
                            <identificacion codigoMision="">
                            </identificacion>
                            <propietario identificadorFiscal="">
                                <nombrePR></nombrePR>
                            </propietario>
                            <tarea idTarea="">
                                <descripcionTarea></descripcionTarea>
                                <areaGeografica>
                                    <centro>
                                        <longitud></longitud>
                                        <latitud></latitud>
                                    </centro>
                                    <profundidad></profundidad>
                                    <forma>esfera</forma>
                                </areaGeografica>
                            </tarea>
                            <dron matriculaDron="">
                                <claseDron></claseDron>
                            </dron>
                            <actuadores>
                                <numeroActuadores></numeroActuadores>
                            </actuadores>
                            <sensores>
                                <numeroSensores></numeroSensores>
                            </sensores>
                        </matricula>
                    </dronTarea>
                </dronesTarea>
            </tarea>
